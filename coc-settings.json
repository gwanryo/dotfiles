{
  "yaml.schemas": {
    "kubernetes": "*.y*ml",
    "https://json.schemastore.org/ansible-playbook": "*ansible*.y*ml"
  },
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"]
    }
  },
  "python.jediPath": "/usr/local/lib/python3.9/site-packages",
  "java.errors.incompleteClasspath.severity": "ignore"
}
